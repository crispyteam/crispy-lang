tree = {
    value: 2,
    left: {
        value: 1,
        left: nil,
        right: nil,
    },
    right: {
        value: 4,
        left: {
            value: 3,
            left: nil,
            right: nil
        },
        right: nil
    }
}

fun traverse(node) {
    if node.left != nil {
        traverse(node.left);
    }
        
    println(node.value);
    
    if node.right != nil {
        traverse(node.right);
    }
}

fun insert(root, newNode) {
    if root.value < newNode.value {
        if root.right != nil {
            insert(root.right, newNode);
        } else {
            root.right = newNode;
        }
    } else {
        if root.left != nil {
            insert(root.left, newNode);
        } else {
            root.left = newNode;
        }
    }
}

traverse(tree);
insert(tree, {value: 0, left: nil, right: nil});
println();
traverse(tree);
insert(tree, {value: 2.5, left: nil, right: nil});
println();
traverse(tree);